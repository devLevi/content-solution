      <mj-accordion border="none" padding="1px" />
      <mj-accordion-element icon-wrapped-url="https://i.imgur.com/IVBjOsN.png" icon-unwrapped-url="https://i.imgur.com/EwElUXT.png" icon-height="24px" icon-width="24px" />
